    "import pandas as pd\n",
    "import numpy as np\n",
    "# Our diamictite data\n",
    "\n",
    "Mozaan_Si_Iso = -1.18\n",
    "Mozaan_Iron = 25\n",
    "Afrikander_Si_Iso = -.25\n",
    "Afrikander_Iron = 8.4\n",
    "Promise_Si_Iso = -.34\n",
    "Promise_Iron = 9.51\n",
